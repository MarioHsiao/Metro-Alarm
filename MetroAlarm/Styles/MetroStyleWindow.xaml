										<DoubleAnimation Duration="0:0:1" From="90" To="0" Storyboard.TargetName="flipper" Storyboard.TargetProperty="RotationY">
											<DoubleAnimation.EasingFunction>
												<BounceEase EasingMode="EaseOut" Bounces="2"/>
											</DoubleAnimation.EasingFunction>
										</DoubleAnimation>
									</Storyboard>
								</VisualState>
								<VisualState x:Name="Closed">
									<Storyboard>
										<DoubleAnimation From="1" To="0.8" Duration="0:0:0.3" Storyboard.TargetProperty="(RenderTransform).(Children)[0].ScaleY" Storyboard.TargetName="ContentRoot"/>
										<DoubleAnimation From="1" To="0.8" Duration="0:0:0.3" Storyboard.TargetProperty="(RenderTransform).(Children)[0].ScaleX" Storyboard.TargetName="ContentRoot"/>
